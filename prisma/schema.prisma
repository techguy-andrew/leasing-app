generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Application {
  id         Int      @id @default(autoincrement())
  status     String
  moveInDate String
  property   String
  unitNumber String
  applicant  String
  email      String
  phone      String
  createdAt  DateTime @default(now())
  updatedAt  DateTime
}
